<group name="netsuite,json,">
  <rule id="138101" level="3">
    <decoded_as>json</decoded_as>
    <field name="email">\.*</field>
    <field name="useragent">\.*</field>
    <description>NetSuite grouped events.</description>
  </rule>
  
  <rule id="138102" level="3">
    <if_sid>138101</if_sid>
    <status>Success</status>
    <description>NetSuite login success event.</description>
  </rule>
  
  <rule id="138103" level="5">
    <if_sid>138101</if_sid>
    <status>Failure</status>
    <description>NetSuite login failure event.</description>
  </rule>
</group>
