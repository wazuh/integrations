<group name="github,dependabot,">
	
	<rule id="100891" level="3">
		<decoded_as>json</decoded_as>
		<match>dependabot</match>
		 <description>Dependabot log appears</description>
	 </rule>
         
         <rule id="100892" level="9">
                <if_sid>100891</if_sid>
                <field name="security_vulnerability.severity">critical</field>
                 <description>Dependabot: Critical severity log appear</description>
         </rule>

         <rule id="100893" level="5">
                <if_sid>100891</if_sid>
                <field name="security_vulnerability.severity">medium</field>
                 <description>Dependabot: Medium severity log appear</description>
         </rule>

        <rule id="100894" level="3">
                <if_sid>100891</if_sid>
                <field name="security_vulnerability.severity">low</field>
                 <description>Dependabot: Low severity log appear</description>
         </rule>

       <rule id="100895" level="7">
                <if_sid>100891</if_sid>
                <field name="security_vulnerability.severity">low</field>
                 <description>Dependabot: High severity log appear</description>
         </rule>


</group>
